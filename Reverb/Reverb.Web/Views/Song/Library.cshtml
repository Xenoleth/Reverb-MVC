@using Reverb.Web.Models.Song
@model ICollection<SongViewModel>
 
<div class="col-md-3">
    @Html.Partial("_SongSearchPartial", new SongSearchViewModel())
</div>

<div class="col-md-9">
    @if (Model.Count == 0)
    {
        <p>No songs were found</p>
    }

    @foreach (var song in Model)
    {
        @Html.Partial("_SongPartial", song);
    }
</div>
